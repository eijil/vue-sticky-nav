{
  "name": "vue-sticky-nav",
  "description": "A mobile friendly sticky nav compontents for Vue.js",
  "version": "1.1.6",
  "author": "jiel1988 <191754186@qq.com>",
  "license": "MIT",
  "private": false,
  "main": "dist/vue-sticky-nav.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules"
  },
  "dependencies": {
    "@tweenjs/tween.js": "^18.3.2",
    "vue": "^2.6.10"
  },
  "browserslist": [
    "Android >= 4.4",
    "iOS >=5",
    "iOS 8"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/eijil/vue-sticky-nav"
  },
  "keywords": [
    "vue-component",
    "Vue",
    "sticky",
    "stickyNav",
    "sticky-nav"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-plugin-pxtoviewport": {
        "viewportWidth": 750
      }
    }
  },
  "devDependencies": {
    "autoprefixer": "^9.7.2",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.4",
    "node-sass": "^4.13.0",
    "postcss-plugin-pxtoviewport": "^0.0.6",
    "sass-loader": "^7.3.1",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  }
}
